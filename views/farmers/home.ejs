<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>
		All Farmers
	</title>
	<link rel="stylesheet" href="">
</head>
<body>
	<h1>All Farmers info</h1>
	<table>
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>Address</th>
				<th>Action</th>
			</tr>
		</thead>
		<tbody>
			<% farmers.forEach(farmer => { %>
				<tr>
					<td><%= farmer.id %></td>
					<td><%= farmer.name %></td>
					<td><%= farmer.address %></td>
					<td><a href="/farmers/<%= farmer.id %>">Show Details</a></td>
				</tr>
			<% })%>
		</tbody>
	</table>
</body>
</html>