<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>
		All Farmers
	</title>
	<link rel="stylesheet" href="">
</head>
<body>
	<h1>All Farmers info</h1>
	<table border=1 width="600">
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th width="30%">Address</th>
				<th>Details</th>
				<th>History</th>
			</tr>
		</thead>
		<tbody>
			<% farmers.forEach(farmer => { %>
				<tr>
					<td><%= farmer.id %></td>
					<td><%= farmer.name %></td>
					<td><%= farmer.address %></td>
					<td><a href="/farmers/<%= farmer.id %>">Current Details</a></td>
					<td><a href="/farmers/<%= farmer.id %>/history">History</a></td>
				</tr>
			<% })%>
		</tbody>
	</table>
	<h4><a href="/">back</a></h4>
</body>
</html>